{
  "deno.enable": true,
  "deno.unstable": [
    "css",
    "html",
    "component"
  ],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "files.exclude": {
    ".git": true,
    ".gitignore": true,
    ".vscode": true,
    "node_modules": true,
    "pnpm-lock.yaml": true,
    ".astro": true,
    "deno.json": true,
    "tsconfig.json": true,
    ".cache": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}